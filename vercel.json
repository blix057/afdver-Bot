{
  "version": 2,
  "builds": [
    { "src": "web/api/*.js", "use": "@vercel/node" },
    { "src": "web/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/api/links$", "dest": "web/api/links.js" },
    { "src": "^/$", "dest": "web/index.html" },
    { "src": "^(.*)$", "dest": "web$1" }
  ]
}
